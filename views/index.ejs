<!-- views/users.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title>Users List</title>
</head>
<body>
    <h1>Users List</h1>
   <form action="/logout" method="POST">
    <button type="submit">Logout</button>
</form>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Mobile No</th>
                <th>Email</th>
                <!-- Add other columns as needed -->
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.first_name %></td>
                    <td><%= user.last_name %></td>
                    <td><%= user.mobile_no %></td>

                    <td><%= user.email %></td>
                    <!-- Add other columns as needed -->
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
